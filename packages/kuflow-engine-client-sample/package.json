{
  "name": "@kuflow/kuflow-engine-client-sample",
  "version": "0.2.0-next.2",
  "private": true,
  "keywords": [
    "node",
    "typescript",
    "kuflow",
    "workflows",
    "temporalio"
  ],
  "scripts": {
    "build": "npm run build:typescript",
    "build:typescript": "tsc --build",
    "build:watch": "npm run clean && npm run build:typescript -- --watch",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "start": "ts-node src/worker.ts",
    "start:watch": "nodemon src/worker.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@kuflow/kuflow-engine-client-activity-kuflow": "0.2.0-next.2",
    "@kuflow/kuflow-engine-client-core": "0.2.0-next.2",
    "@kuflow/kuflow-rest-client": "0.0.2-4",
    "@temporalio/activity": "1.4.4",
    "@temporalio/client": "1.4.4",
    "@temporalio/worker": "1.4.4",
    "@temporalio/workflow": "1.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
