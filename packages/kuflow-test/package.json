{
  "name": "@kuflow/kuflow-test",
  "version": "0.2.0-next.3",
  "private": true,
  "keywords": [
    "node",
    "typescript",
    "kuflow",
    "workflows",
    "temporalio"
  ],
  "homepage": "https://github.com/kuflow/kuflow-sdk-js",
  "bugs": {
    "url": "https://github.com/kuflow/kuflow-sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kuflow/kuflow-sdk-js.git"
  },
  "license": "MIT",
  "author": "KuFlow S.L.",
  "scripts": {
    "build": "npm run build:typescript",
    "build:typescript": "tsc --build",
    "build:watch": "npm run clean && npm run build:typescript -- --watch",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "start": "ts-node src/worker.ts",
    "start:watch": "nodemon src/worker.ts",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@kuflow/kuflow-temporal-activity-kuflow": "0.2.0-next.3",
    "@kuflow/kuflow-temporal-core": "0.2.0-next.3",
    "@kuflow/kuflow-rest": "0.2.0-next.3",
    "@temporalio/activity": "1.4.4",
    "@temporalio/client": "1.4.4",
    "@temporalio/worker": "1.4.4",
    "@temporalio/workflow": "1.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
