{
  "name": "@kuflow/kuflow-rest-generator",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autorest": "3.7.1"
  },
  "scripts": {
    "autorest": "autorest",
    "generate": "npm run generate:autorest && npm run generate:workaround && npm run generate:format",
    "generate:autorest": "AUTOREST_HOME=$(pwd) autorest --version=3.10.2 readme.md",
    "generate:workaround": "scripts/fix-generated-code.js",
    "generate:format": "cd ../../../ && npm run format:write"
  }
}
